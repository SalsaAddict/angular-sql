  </connectionStrings>
  <appSettings>
    <clear />
    <add key="SaltLength" value="24" />
    <add key="HashLength" value="24" />
    <add key="PBKDF2IterationCount" value="1000" />
    <add key="JsonWebTokenKey" value="3dd!370rr3$7h3M@mb0K!ngFr0mN3wY0rk" />
